<div class="chat-container flex column">
  <div id="dom-messages">
    {{#each chat.messages}}
    <div class="message">
      {{user}}
      {{message}}
    </div>
    {{/each}}
  </div>
  <div class="input">
    <input type="text" id="new-message">
    <button id="send-button">Send</button>
  </div>
</div>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="../../javascripts/chat.js"></script>